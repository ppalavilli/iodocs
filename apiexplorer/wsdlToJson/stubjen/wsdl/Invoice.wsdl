<?xml version="1.0" encoding="UTF-8"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:types="http://svcs.paypal.com/types/pt" xmlns:common="http://svcs.paypal.com/types/common" xmlns:services="http://svcs.paypal.com/services" xmlns:ns="http://svcs.paypal.com/services" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" ns:version="1.6.0" targetNamespace="http://svcs.paypal.com/services">

	<wsdl:types>
		<xs:schema xmlns:annox="http://annox.dev.java.net" xmlns:ov="http://annox.dev.java.net/net.sf.oval.constraint" xmlns:sec="http://annox.dev.java.net/com.paypal.foundation.security.annotation" xmlns:ppv="http://annox.dev.java.net/com.paypal.foundation.validation.oval" xmlns:ppc="http://annox.dev.java.net/com.paypal.foundation.validation.oval.constraint" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://svcs.paypal.com/types/common">

			<xs:simpleType name="AckCode">
				<xs:annotation>
					<xs:documentation>
						AckCodeType This code identifies the
						acknowledgment code types that could be used to
						communicate the status of processing a (request)
						message to an application. This code would be
						used as part of a response message that contains
						an application level acknowledgment element.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Success">
						<xs:annotation>
							<xs:documentation>
								Request processing succeeded.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Failure">
						<xs:annotation>
							<xs:documentation>
								Request processing failed.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Warning">
						<xs:annotation>
							<xs:documentation>
								Request processing completed with
								warning information being included in
								the response message.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="SuccessWithWarning">
						<xs:annotation>
							<xs:documentation>
								Request processing completed successful
								with some with some warning information
								that could be useful for the requesting
								application to process and/or record.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="FailureWithWarning">
						<xs:annotation>
							<xs:documentation>
								Request processing failed with some
								error and warning information that
								requesting application should process to
								determine cause(s) of failure.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>



			<xs:complexType name="BaseAddress">
				<xs:sequence maxOccurs="1" minOccurs="1">
					<xs:element name="line1" type="xs:string" maxOccurs="1" minOccurs="1">
						
					</xs:element>
					<xs:element name="line2" type="xs:string" maxOccurs="1" minOccurs="0">
						
					</xs:element>	
					<xs:element name="city" type="xs:string" maxOccurs="1" minOccurs="1">
						
					</xs:element>
					<xs:element name="state" type="xs:string" maxOccurs="1" minOccurs="0">
						
					</xs:element>
					<xs:element name="postalCode" type="xs:string" maxOccurs="1" minOccurs="0">
						
					</xs:element>
					<xs:element name="countryCode" type="xs:string" maxOccurs="1" minOccurs="1">
						
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
							
					</xs:element>					
					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ClientDetailsType">
				<xs:annotation>
					<xs:documentation>
						Details about the end user of the application
						invoking this service.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="0" maxOccurs="1" name="ipAddress" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="deviceId" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="applicationId" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="model" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="geoLocation" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="customerType" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="partnerName" type="xs:string"/>
					<xs:element minOccurs="0" maxOccurs="1" name="customerId" type="xs:string"/>
					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="CurrencyType">
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="1" name="code" type="xs:string"/>
					<xs:element minOccurs="1" maxOccurs="1" name="amount" type="xs:decimal"/>
				</xs:sequence>
			</xs:complexType>

			<xs:simpleType name="ReferrerCode">
				<xs:annotation>
					<xs:documentation>
						BN code for tracking transactions with a particular partner.
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:maxLength value="32"/>
				</xs:restriction>
			</xs:simpleType>
	
			<xs:simpleType name="DayOfWeek">
				<xs:restriction base="xs:string">
					<xs:enumeration value="NO_DAY_SPECIFIED"/>
					<xs:enumeration value="SUNDAY"/>
					<xs:enumeration value="MONDAY"/>
					<xs:enumeration value="TUESDAY"/>
					<xs:enumeration value="WEDNESDAY"/>
					<xs:enumeration value="THURSDAY"/>
					<xs:enumeration value="FRIDAY"/>
					<xs:enumeration value="SATURDAY"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="DetailLevelCode">
				<xs:annotation>
					<xs:documentation>
						DetailLevelCodeType
					</xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="ReturnAll">
						<xs:annotation>
							<xs:documentation>
								Return in response message all detail
								levels.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="ErrorCategory">
				<xs:restriction base="xs:string">
					<xs:enumeration value="System"/>
					<xs:enumeration value="Application"/>
					<xs:enumeration value="Request"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="ErrorData">
				<xs:annotation>
					<xs:documentation>
						This type contains the detailed error
						information resulting from the service
						operation.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="errorId" type="xs:long"/>
					<xs:element name="domain" type="xs:string"/>
					<xs:element minOccurs="0" name="subdomain" type="xs:string"/>
					<xs:element name="severity" type="common:ErrorSeverity"/>
					<xs:element name="category" type="common:ErrorCategory"/>
					<xs:element name="message" type="xs:string"/>
					<xs:element minOccurs="0" name="exceptionId" type="xs:token"/>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="parameter" nillable="true" type="common:ErrorParameter"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ErrorParameter">
				<xs:simpleContent>
					<xs:extension base="xs:string">
						<xs:attribute name="name" type="xs:string"/>
					</xs:extension>
				</xs:simpleContent>
			</xs:complexType>

			<xs:simpleType name="ErrorSeverity">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Error"/>
					<xs:enumeration value="Warning"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="FaultMessage">
				<xs:annotation>
					<xs:documentation>
						This specifies a fault, encapsulating error
						data, with specific error codes.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" name="responseEnvelope" type="common:ResponseEnvelope"/>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="error" nillable="true" type="common:ErrorData"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="PhoneNumberType">
				<xs:sequence maxOccurs="1" minOccurs="1">
					<xs:element minOccurs="1" maxOccurs="1" name="countryCode" type="xs:string">
						
					</xs:element>
					<xs:element minOccurs="1" maxOccurs="1" name="phoneNumber" type="xs:string">
						
					</xs:element>	
					<xs:element minOccurs="0" maxOccurs="1" name="extension" type="xs:string">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="RequestEnvelope">
				<xs:annotation>
					<xs:documentation>
						This specifies the list of parameters with every
						request to the service.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="detailLevel" type="common:DetailLevelCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								This specifies the required detail level
								that is needed by a client application
								pertaining to a particular data
								component (e.g., Item, Transaction,
								etc.). The detail level is specified in
								the DetailLevelCodeType which has all
								the enumerated values of the detail
								level for each component.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="errorLanguage" type="xs:string" minOccurs="1" default="en_US">
						<xs:annotation>
							<xs:documentation>
								This should be the standard RFC 3066
								language identification tag, e.g.,
								en_US.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="ResponseEnvelope">
				<xs:annotation>
					<xs:documentation>
						This specifies a list of parameters with every
						response from a service.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="1" name="timestamp" type="xs:dateTime"/>
					<xs:element name="ack" minOccurs="1" type="common:AckCode">
						<xs:annotation>
							<xs:documentation>
								Application level acknowledgment code.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="1" maxOccurs="1" name="correlationId" type="xs:string"/>
					<xs:element minOccurs="1" maxOccurs="1" name="build" type="xs:string"/>
					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>

			<xs:element name="FaultMessage" nillable="true" type="common:FaultMessage"/>

		</xs:schema>

		<xs:schema xmlns:inv="http://svcs.paypal.com/types/pt" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:annox="http://annox.dev.java.net" xmlns:ov="http://annox.dev.java.net/net.sf.oval.constraint" xmlns:sec="http://annox.dev.java.net/com.paypal.foundation.security.annotation" xmlns:ppm="http://annox.dev.java.net/com.paypal.foundation.logging" xmlns:ppv="http://annox.dev.java.net/com.paypal.foundation.validation.oval" xmlns:ppvc="http://annox.dev.java.net/com.paypal.foundation.validation.oval.constraint" attributeFormDefault="unqualified" elementFormDefault="unqualified" jaxb:version="2.1" targetNamespace="http://svcs.paypal.com/types/pt">
		<!-- NOTE: this schema target namespace will eventually be changed to http://svcs.paypal.com/types/invoice -->

			<!--  jaxb:extensionBindingPrefixes="annox" -->

			<xs:import namespace="http://svcs.paypal.com/types/common"/>

			<!-- ###################################################### -->
			<!-- Shared invoice related types definition, alphabetized  -->
			<!-- ###################################################### -->

			<xs:complexType name="BusinessInfoType">
				
				<xs:annotation>
					<xs:documentation>
						Contact information for a company participating in the invoicing system.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="firstName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								First name of the company contact.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="lastName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last name of the company contact.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="businessName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Business name of the company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="phone" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Phone number for contacting the company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="fax" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Fax number used by the company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="website" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Website used by the company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="customValue" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Custom value to be displayed in the contact information details.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="address" type="common:BaseAddress" maxOccurs="1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Street address of the company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:simpleType name="PaymentTermsType">
				<xs:annotation>
					<xs:documentation>
						Specifies the payment terms for this invoice.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="DueOnReceipt">
						<xs:annotation>
							<xs:documentation>
								Due when the payer recieves the invoice.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DueOnDateSpecified">
						<xs:annotation>
							<xs:documentation>
								Due on the date specified elsewhere in the invoice.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Net10">
						<xs:annotation>
							<xs:documentation>
								Due ten days from the invoice date.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Net15">
						<xs:annotation>
							<xs:documentation>
								Due fifteen days from the invoice date.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Net30">
						<xs:annotation>
							<xs:documentation>
								Due thirty days from the invoice date.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Net45">
						<xs:annotation>
							<xs:documentation>
								Due forty five days from the invoice date.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			
			<xs:simpleType name="PaymentMethodsType">
				<xs:annotation>
					<xs:documentation>
						Specifies the payment methods that can be used to mark an invoice as paid.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="BankTransfer">
						<xs:annotation>
							<xs:documentation>
								Payment done by a bank transfer.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Cash">
						<xs:annotation>
							<xs:documentation>
								Payment done by cash.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Check">
						<xs:annotation>
							<xs:documentation>
								Payment done by a check.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>					
					<xs:enumeration value="CreditCard">
						<xs:annotation>
							<xs:documentation>
								Payment done by a credit card.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DebitCard">
						<xs:annotation>
							<xs:documentation>
								Payment done by a debit card.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="Other">
						<xs:annotation>
							<xs:documentation>
								Payment done by another method not specified in this enumeration.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>					
					<xs:enumeration value="PayPal">
						<xs:annotation>
							<xs:documentation>
								Payment done by PayPal.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="WireTransfer">
						<xs:annotation>
							<xs:documentation>
								Payment done by a wire transfer.
							</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			
					
			<xs:complexType name="InvoiceItemType">
				<xs:annotation>
					<xs:documentation>
						Item information about a service or product listed in the invoice.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								SKU or item name.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Description of the item.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the product or service was provided.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="quantity" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Item count.
							</xs:documentation>
								
						</xs:annotation>
					</xs:element>
					<xs:element name="unitPrice" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Price of the item, in the currency specified by the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="taxName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Name of an applicable tax, if any.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="taxRate" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Rate of an applicable tax, if any.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>
			
			<xs:complexType name="InvoiceItemListType">
				<xs:annotation>
					<xs:documentation>
						A list of invoice items.
					</xs:documentation>
					
				</xs:annotation>			
				<xs:sequence>
					<xs:element name="item" type="inv:InvoiceItemType" minOccurs="1" maxOccurs="unbounded">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="InvoiceType">
				<xs:annotation>
					<xs:documentation>
						Invoice details about the merchant, payer, totals and terms.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="merchantEmail" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Merchant's email.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="payerEmail" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Email to which the invoice will be sent.
							</xs:documentation>
                            
						</xs:annotation>
					</xs:element>
					<xs:element name="number" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Unique identifier for the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="merchantInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Company contact information of the merchant company sending the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="itemList" type="inv:InvoiceItemListType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								List of items included in this invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="currencyCode" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Currency used for all invoice item amounts and totals.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="invoiceDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice will be enabled.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dueDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice payment is due.
							</xs:documentation>
								
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentTerms" type="inv:PaymentTermsType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Terms by which the invoice payment is due.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="discountPercent" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A discount percent applied to the invoice, if any.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="discountAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A discount amount applied to the invoice, if any.  If DiscountPercent is provided, DiscountAmount is ignored.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="terms" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								General terms for the invoice.
							</xs:documentation>
								
						</xs:annotation>
					</xs:element>
					<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Note to the payer company.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="merchantMemo" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Memo for book keeping that is private to the Merchant.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="billingInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Billing information for the payer.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Shipping information for the payer.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Cost of shipping.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingTaxName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Name of the applicable tax on shipping cost, if any.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingTaxRate" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Rate of the applicable tax on shipping cost, if any.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					
					
					
					<xs:element name="logoUrl" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The external image URL of the invoice's logo, if any
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>	
					<xs:element name="referrerCode" type="common:ReferrerCode" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								BN code for tracking transactions with a particular partner. 
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>					
					
                    <xs:element name="customAmountLabel" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Label used to display custom amount value.
                                If a value is entered for customAmountLabel, then customAmountValue cannot be empty.
                            </xs:documentation>
                            
                        </xs:annotation>
                    </xs:element>
                    
                    <xs:element name="customAmountValue" type="xs:decimal" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Value of custom amount.
                                If a value is entered for customAmountValue, then customAmountLabel cannot be empty.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                 
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>


			<xs:simpleType name="StatusType">
				<xs:annotation>
					<xs:documentation>
						Specifies the invoice status.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Draft"/>
					<xs:enumeration value="Sent"/>
					<xs:enumeration value="Paid"/>
					<xs:enumeration value="MarkedAsPaid"/>
					<xs:enumeration value="Canceled"/>
					<xs:enumeration value="Refunded"/>
					<xs:enumeration value="PartiallyRefunded"/>
					<xs:enumeration value="MarkedAsRefunded"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="OriginType">
				<xs:annotation>
					<xs:documentation>
						Specifies the merchant or payer.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Web"/>
					<xs:enumeration value="API"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="ActorType">
				<xs:annotation>
					<xs:documentation>
						Specifies the merchant or payer.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Merchant"/>
					<xs:enumeration value="Payer"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="InvoiceDetailsType">
				<xs:annotation>
					<xs:documentation>
						Invoice details about the invoice status and state change dates.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="status" type="inv:StatusType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Status of the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="totalAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The total amount of the invoice (cost of items, shipping and tax, less any discount).
								This field is set by the invoicing system and will ignore any changes made by API callers.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="origin" type="inv:OriginType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Whether the invoice was created via the website or via an API call.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="createdDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was created.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="createdBy" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account that created the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="canceledDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								If canceled, date when the invoice was canceled.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="canceledByActor" type="inv:ActorType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Actor who canceled the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="canceledBy" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account that canceled the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="lastUpdatedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was last edited.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lastUpdatedBy" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account that last edited the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="firstSentDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was first sent.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lastSentDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was last sent.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lastSentBy" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account that last sent the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="paidDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								If the invoice was paid, date when the invoice was paid.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>
            <xs:complexType name="OtherPaymentRefundDetailsType">
                <xs:annotation>
                    <xs:documentation>
                        Details of the refund made against this invoice.
                    </xs:documentation>
                    
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
							<xs:documentation>
								Optional note associated with the refund.
							</xs:documentation>
                            
						</xs:annotation>
                    </xs:element>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Date when the invoice was marked as refunded. If the date is not specified, the current date and time is used as a default.
                                In addition, the date must be after the payment date of the invoice.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
                </xs:sequence>
            </xs:complexType>

             <xs:complexType name="PayPalPaymentRefundDetailsType">
                <xs:annotation>
                    <xs:documentation>
                        Details of the paypal refund made against this invoice.
                    </xs:documentation>
                    
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Date when the invoice was marked as refunded by PayPal.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="PayPalPaymentDetailsType">
				<xs:annotation>
					<xs:documentation>
						PayPal payment details about the invoice.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="transactionID" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Transaction ID of the PayPal payment.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was paid.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:simpleType name="OtherPaymentMethodType">
				<xs:annotation>
					<xs:documentation>
						Specifies the type of payment.
					</xs:documentation>
					
				</xs:annotation>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Bank_transfer"/>
					<xs:enumeration value="Cash"/>
					<xs:enumeration value="Check"/>
					<xs:enumeration value="Credit_card"/>
					<xs:enumeration value="Debit_card"/>
					<xs:enumeration value="PayPal"/>
					<xs:enumeration value="Wire_transfer"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="OtherPaymentDetailsType">
				<xs:annotation>
					<xs:documentation>
						Offline payment details about the invoice.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="method" type="inv:PaymentMethodsType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Method used for the offline payment.
							</xs:documentation>						
						</xs:annotation>
					</xs:element>
					<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Optional note associated with the payment.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date when the invoice was paid.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="PaymentDetailsType">
				<xs:annotation>
					<xs:documentation>
						Payment details about the invoice.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="viaPayPal" type="xs:boolean" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								True if the invoice was paid using PayPal.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="paypalPayment" type="inv:PayPalPaymentDetailsType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								PayPal payment details.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="otherPayment" type="inv:OtherPaymentDetailsType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Other payment details.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="DateRangeType">
				<xs:annotation>
					<xs:documentation>
						Determines an inclusive date range.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="startDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Start of the date range.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="endDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								End of the date range.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="SearchParametersType">
				<xs:annotation>
					<xs:documentation>
						Search parameters criteria.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Email search string.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="recipientName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Recipient search string.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="businessName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Company search string.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="invoiceNumber" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice number search string.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="status" type="inv:StatusType" minOccurs="0" maxOccurs="10">
						<xs:annotation>
							<xs:documentation>
								Invoice status search.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lowerAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice amount search.  Specifies the smallest amount to be returned.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="upperAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice amount search.  Specifies the largest amount to be returned.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="currencyCode" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Currency used for lower and upper amounts.  Required when lowerAmount or upperAmount is specified.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="memo" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice memo search string.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="origin" type="inv:OriginType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Whether the invoice was created via the website or via an API call.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="invoiceDate" type="inv:DateRangeType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice date range filter.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dueDate" type="inv:DateRangeType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice due date range filter.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentDate" type="inv:DateRangeType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice payment date range filter.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="creationDate" type="inv:DateRangeType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice creation date range filter.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="InvoiceSummaryType">
				<xs:annotation>
					<xs:documentation>
						Summary of invoice information.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="invoiceID" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The created invoice's ID.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="merchantEmail" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice creator's email.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="payerEmail" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Email to which the invoice will be sent.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="number" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Unique identifier for the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="billingBusinessName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Business name of the billing info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="billingFirstName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								First name of the billing info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="billingLastName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last name of the billing info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingBusinessName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Business name of the shipping info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingFirstName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								First name of the shipping info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingLastName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last name of the shipping info.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="totalAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Total amount of the invoice.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="currencyCode" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Currency used for all invoice item amounts and totals.
							</xs:documentation>
							
						</xs:annotation>
					</xs:element>
					<xs:element name="invoiceDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice will be enabled.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dueDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice payment is due.
							</xs:documentation>
						</xs:annotation>
